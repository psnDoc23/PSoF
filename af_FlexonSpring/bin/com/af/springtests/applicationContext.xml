<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
		
	<bean id="guestListService"
		class="com.af.flexonspring.chapter5.beanfactory.services.GuestListServiceImpl">
		<property name="guestList">
			<list>
				<value>Guest One</value>
				<value>Guest Two</value>
				<value>Guest Three</value>
				<value>Guest Four</value>
			</list>
		</property>
	</bean>
</beans>